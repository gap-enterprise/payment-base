    <createView fullDefinition="false" viewName="pay_reference_document_view">SELECT rd.id,
      rd.type_id,
      rd.date,
      rd.reference,
      rd.object,
      rd.place,
      rd.amount,
      rd.deposit_date,
      rd.entry_date,
      rd.issuer_id,
      rd.other_reference,
      rd.status_id,
      rd.step_id,
      rd.amount_paid,
      rd.amount_left,
      ps.name AS name_issuer,
      tp.abbreviated AS abbreviated_issuer,
      tp.code AS code_issuer
      FROM ((pay_reference_document rd
      LEFT JOIN pay_third_party tp ON ((tp.id = rd.issuer_id)))
      LEFT JOIN ad_person ps ON ((ps.id = rd.issuer_id)));</createView>
  </changeSet>
